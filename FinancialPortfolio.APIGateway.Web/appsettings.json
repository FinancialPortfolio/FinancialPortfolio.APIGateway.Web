{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        },
        "Properties": {
            "Application": "Web Gateway"
        }
    },
    "MessagingSettings": {
        "GroupId": "gateway-web-consumer-group"
    },
    "OutboxOptions": {
        "Enabled": true,
        "IntervalMilliseconds": 5000
    },
    "MongoSettings": {
        "DatabaseName": "WebGateway"
    },
    "AuthenticationSettings": {
        "Audience": "FPGateway"
    },
    "swagger": {
        "enabled": true,
        "reDocEnabled": false,
        "name": "v1",
        "title": "API",
        "version": "v1",
        "routePrefix": "swagger",
        "includeSecurity": true
    },
    "AllowedHosts": "*"
}
